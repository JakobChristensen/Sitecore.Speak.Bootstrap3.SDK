      <Bootstrap3-NavBar Active="Bottom" Id="NavBar3" BrandText="Brand" HasInnerContainer="True" ItemsDataSource="/sitecore/client/Bootstrap3/Content/Bootstrap3/Components/PageSettings/Menu" />
