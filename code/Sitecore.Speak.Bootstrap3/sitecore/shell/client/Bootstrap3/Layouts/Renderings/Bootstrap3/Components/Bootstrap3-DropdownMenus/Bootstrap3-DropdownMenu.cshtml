@using System.Web.Mvc
@using Sitecore.Bootstrap
@using Sitecore.Mvc
@model Sitecore.Shell.Client.Bootstrap3.Layouts.Renderings.Bootstrap3.Components.Bootstrap3DropdownMenus.Bootstrap3DropdownMenuRenderingModel
@{
  Model.Class = "dropdown-menu";
  var items = BootstrapApplication.GetMenuItems(Model, Model.ItemsDataSource, Model.ControlId).ToList();
  if (!items.Any())
  {
    return;
  }
}              
<ul @Model.HtmlAttributes role="menu">
  @Html.Sitecore().Menu(items, Model.DisplayFieldName, Model.IconFieldName, Model.ClickFieldName);
</ul>
